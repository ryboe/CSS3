  border-style:
    - include: line-style

  border-width:
    - include: line-width

  box:
    - match: '\b(?:padding-box|content-box|border-box){{b}}'
      scope: support.constant.property-value.css

  caret-shape:
    - match: '\b(?:underscore|block|bar|auto){{b}}'
      scope: support.constant.property-value.css

  color:
